  </xs:complexType>

  <xs:complexType name="MouseClickHandlerType">
    <xs:annotation>
      <xs:documentation>A tool for models of a map container to handle mouse click events</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="mb:AbstractToolType"/>
    </xs:complexContent>
