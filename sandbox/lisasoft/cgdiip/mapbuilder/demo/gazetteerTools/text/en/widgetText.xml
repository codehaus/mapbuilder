<?xml version="1.0" encoding="utf-8" standalone="no" ?>
<WidgetText xmlns="http://mapbuilder.sourceforge.net/mapbuilder" 
    version="0.2.1" id="mbDefaultText" xml:lang="en">
  <!--
    $Id$
  -->
  <!-- User specific widget (XSL) messages -->
  <widgets>
    <Widget>
      <selectPlace> Select a place:</selectPlace>
      <placeNameCoords>Coordinates of selected place name:</placeNameCoords>
      <transfer>Transfer</transfer>
      <close>Close</close>
      <postalCodeCoords>Coordinates of selected postal code:</postalCodeCoords>
      <postalCodeRegionTitle>Postal code region:</postalCodeRegionTitle>
    </Widget>
    <PlaceNameForm>
      <title>Place Name Search</title>
      <placeName>Place name:</placeName>
      <find>Find</find>
      <country>Country:</country>
      <example>Example: Ottawa</example>
      <exampleWorld>Example: Paris, France</exampleWorld>
    </PlaceNameForm>
    <PostalCodeForm>
      <title>Postal Code</title>
      <postalCode>Postal code:</postalCode>
      <find>Find</find>
      <example>Example: M5W 1E6, or just the Forward Sorting Area (FSA) M5W</example>
    </PostalCodeForm>
    <AoiForm>
      <title>Area of interest coordinates (this is a MapBuilder widget)</title>
      <north>North:</north>
      <south>South:</south>
      <east>East:</east>
      <west>West:</west>
    </AoiForm>
    <TabbedContent>
      <canadaPlaceNameForm>Canada</canadaPlaceNameForm>
      <worldPlaceNameForm>World</worldPlaceNameForm>
    </TabbedContent>
  </widgets>
</WidgetText>
