        <DCPType>
          <HTTP>
            <Get onlineResource="http://localhost:8080/geoserver/wfs?"/>
          </HTTP>
        </DCPType>
        <DCPType>
          <HTTP>
            <Post onlineResource="http://localhost:8080/geoserver/wfs?"/>
          </HTTP>
        </DCPType>
      </Transaction>
      <LockFeature>
        <DCPType>
          <HTTP>
            <Get onlineResource="http://localhost:8080/geoserver/wfs?"/>
          </HTTP>
        </DCPType>
        <DCPType>
          <HTTP>
            <Post onlineResource="http://localhost:8080/geoserver/wfs?"/>
          </HTTP>
        </DCPType>
      </LockFeature>
      <GetFeatureWithLock>
        <ResultFormat>
          <GML2/>
        </ResultFormat>
        <DCPType>
          <HTTP>
            <Get onlineResource="http://localhost:8080/geoserver/wfs?"/>
          </HTTP>
        </DCPType>
        <DCPType>
          <HTTP>
            <Post onlineResource="http://localhost:8080/geoserver/wfs?"/>
          </HTTP>
        </DCPType>
      </GetFeatureWithLock>
    </Request>
