  <widgets>
    <AoiForm>
      <east>Est:</east>
      <north>Nord:</north>
      <south>Sud:</south>
      <title>Coordinate dell'area di interesse</title>
      <west>Ovest:</west>
    </AoiForm>
    
    <Back>
      <tooltip>Zoom precedente</tooltip>
    </Back>
    
    <CollectionList>
      <title>Seleziona la mappa da caricare:</title>
    </CollectionList>
    
    <CursorTrack>
      <latitude>lat:</latitude>
      <longitude>lon:</longitude>
      <xcoord>x:</xcoord>
      <ycoord>y:</ycoord>
    </CursorTrack>
    
    <DeleteFeature>
      <tooltip>Elimina l'oggetto o gli oggetti correnti</tooltip>
    </DeleteFeature>
    
    <DragPan>
      <tooltip>Clicca e trascina il mouse per spostare la mappa</tooltip>
    </DragPan>
    
    <EditLine>
      <tooltip>Clicca sulla mappa per aggiungere un punto alla linea</tooltip>
    </EditLine>
    
    <EditPoint>
      <tooltip>Clicca sulla mappa per creare un nuovo punto</tooltip>
    </EditPoint>
    
    <Forward>
      <tooltip>Zoom successivo</tooltip>
    </Forward>
    
    <GetFeatureInfo>
      <tooltip>Clicca sull'oggetto per ulteriori informazioni</tooltip>
    </GetFeatureInfo>	
    
    <InsertFeature>
      <tooltip>Salva l'oggetto o gli oggetti correnti</tooltip>
    </InsertFeature>
    
    <Legend>
      <title>Tematismi</title>
    </Legend>
    
    <Measurement>
      <tooltip>click on the map to measure the distance</tooltip> <!-- TODO: translate -->
    </Measurement>
    
    <ModelStatus>
      <title>Status for:</title>
    </ModelStatus>
    
    <ModelUrlInput>
      <load>load</load>
      <title>URL:</title>
    </ModelUrlInput>
    
    <ProcessArgumentsForm>
    </ProcessArgumentsForm>
    
    <Reset>
      <tooltip>Ritorna alla mappa iniziale</tooltip>
    </Reset>
    
    <Save>
      <tooltip>Salva</tooltip>
    </Save>
    
    <SelectProcess>
    </SelectProcess>
    
    <SetAoi>
      <tooltip>Clicca e trascina per selezionare l'area di interesse</tooltip>
    </SetAoi>
    
    <ShowProcessResult>
      <tabLabel>4. Access results</tabLabel>
    </ShowProcessResult>
    
    <TabbedContent>
      <argsInput>3. Set arguments</argsInput> <!-- TODO: translate -->
      <results>4. Access results</results> <!-- TODO: translate -->
      <selectProcess>2. Select a process</selectProcess> <!-- TODO: translate -->
      <urlInput>1. Select a service</urlInput> <!-- TODO: translate -->
    </TabbedContent>
    
    <Timestamp>
      <title>Data/ora:</title>
    </Timestamp>
    
    <WebServiceForm>
      <mapsheet>NTS index:</mapsheet>
      <ntsTitle>NTS Lookup web service</ntsTitle>
    </WebServiceForm>
    
    <WfsGetFeature>
      <tooltip>Clicca sull'oggetto per interrogarlo</tooltip>
    </WfsGetFeature>
    
    <Widget>
      <abstract>Sommario</abstract>
      <east>Est:</east>
      <moreInfo>Informazioni aggiuntive</moreInfo>
      <north>Nord:</north>
      <searchFormTitle>Coordinate area di interesse</searchFormTitle>
      <south>Sud:</south>
      <west>Ovest:</west>
    </Widget>
    
    <ZoomIn>
      <tooltip>Clicca e trascina il mouse per ingrandire</tooltip>
    </ZoomIn>
    
    <ZoomOut>
      <tooltip>Clicca per ridurre</tooltip>
    </ZoomOut>
  </widgets>
</WidgetText>
