  <!-- Widget (XSL) messages -->
  <widgets>
    <AoiForm>
      <east>Est:</east>
      <north>Nord:</north>
      <south>Sud:</south>
      <title>Coordonées de la région d'intéret</title>
      <west>Ouest:</west>
    </AoiForm>
    
    <Back>
      <tooltip>Changez pour l'extension précédent</tooltip>
    </Back>
    
    <CollectionList>
      <title>Choisir une carte pour regardez:</title>
    </CollectionList>
    
    <CursorTrack>
      <latitude>lat:</latitude>
      <longitude>lon:</longitude>
      <xcoord>x:</xcoord>
      <ycoord>y:</ycoord>
    </CursorTrack>
    
    <DeleteFeature>
      <tooltip>Éffacer la(es) caractéristique(s) courantes sur le serveur</tooltip>
    </DeleteFeature>
    
    <DragPan>
      <tooltip>Cliquer et faire glisser la souris pour voir une autre partie de la carte</tooltip>
    </DragPan>
    
    <EditLine>
      <tooltip>Cliquer pour créer un object géographique linéaire</tooltip>
    </EditLine>
    
    <EditPoint>
      <tooltip>Cliquer pour créer un object géographique de point</tooltip>
    </EditPoint>
    
    <Forward>
      <tooltip>Changez pour l'extension suivante</tooltip>
    </Forward>
    
    <GetFeatureInfo>
      <tooltip>Cliquer pour obtenir plus d'information pour cette couche</tooltip>
    </GetFeatureInfo>
    
    <InsertFeature>
      <tooltip>Sauvegarder la(es) caractéristique(s) courants sur le serveur</tooltip>
    </InsertFeature>
    
    <Legend>
      <title>Couches de la carte</title>
    </Legend>
    
    <Measurement>
      <tooltip>Cliquer sur le plan pour mésurer la distance</tooltip>
    </Measurement>
    
    <ModelStatus>
      <title>Statut de:</title>
    </ModelStatus>
    
    <ModelUrlInput>
      <load>téléchargez</load>
      <title>URL:</title>
    </ModelUrlInput>
    
    <ProcessArgumentsForm>
    </ProcessArgumentsForm>
    
    <Reset>
      <tooltip>Redonner la carte ses dimensions complètes</tooltip>
    </Reset>
    
    <Save>
      <tooltip>Cliquer pour sauvegarder</tooltip>
    </Save>
    
    <SelectProcess>
    </SelectProcess>
    
    <SetAoi>
      <tooltip>Cliquer et faire glisser la souris pour positionner une AOI</tooltip>
    </SetAoi>
    
    <ShowProcessResult>
      <tabLabel>4. Accéder aux résultats</tabLabel>
    </ShowProcessResult>
    
    <TabbedContent>
      <argsInput>3. Placer arguments</argsInput>
      <results>4. Accéder aux résultats</results>
      <selectProcess>2. Choisir un procés</selectProcess>
      <urlInput>1. Choisir un service</urlInput>
    </TabbedContent>
    
    <Timestamp>
      <title>Date de l'image:</title>
    </Timestamp>
    
    <WebServiceForm>
      <mapsheet>Code SNRC:</mapsheet>
      <ntsTitle>Service de recherche par SNRC</ntsTitle>
    </WebServiceForm>
    
    <WfsGetFeature>
      <tooltip>Cliquer sur la caractéristique pour la questionner</tooltip>
    </WfsGetFeature>
    
    <Widget>
      <abstract>Résumé</abstract>
      <east>Est:</east>
      <moreInfo>Lisez à propos de cette carte</moreInfo>
      <north>Nord:</north>
      <searchFormTitle>Région d'intérêt</searchFormTitle>
      <south>Sud:</south>
      <west>Ouest:</west>
    </Widget>
    
    <ZoomIn>
      <tooltip>Cliquer et faire glisser la souris pour agrandir</tooltip>
    </ZoomIn>
    
    <ZoomOut>
      <tooltip>Cliquer pour réduire</tooltip>
    </ZoomOut>
  </widgets>
  
