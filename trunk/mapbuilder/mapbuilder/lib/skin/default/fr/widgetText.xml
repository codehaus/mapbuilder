<?xml version="1.0" encoding="utf-8" standalone="no" ?>
<WidgetText xmlns="http://mapbuilder.sourceforge.net/mapbuilder" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://mapbuilder.sourceforge.net/mapbuilder /mapbuilder/lib/schemas/config.xsd"
    version="0.2.1" id="mbDefaultText" xml:lang="fr">
  <!--
    
    Description: This file contains all text used in widgets for multilingual 
                  support.  The text values are set as XSL params before the 
                  widget is painted.
    Licence:     LGPL as per: http://www.gnu.org/copyleft/lesser.html 

    $Id$
  -->
  <widgets>
    <AoiForm>
      <east>Est:</east>
      <north>Nord:</north>
      <south>Sud:</south>
      <title>Coordonées de la région d`intéret</title>
      <west>Ouest:</west>
    </AoiForm>
    
    <Back>
      <tooltip>get the previous extent</tooltip> <!-- TODO: translate -->
    </Back>
    
    <CollectionList>
      <title>Choisir une carte pour regardez:</title>
    </CollectionList>
    
    <CursorTrack>
      <latitude>lat:</latitude>
      <longitude>lon:</longitude>
      <xcoord>x:</xcoord>
      <ycoord>y:</ycoord>
    </CursorTrack>
    
    <DeleteFeature>
      <tooltip>Delete the current Feature(s) on the server</tooltip> <!-- TODO: translate -->
    </DeleteFeature>
    
    <DragPan>
      <tooltip>cliquer et faire glisser la souris pour voir un autre parti de la carte</tooltip>
    </DragPan>
    
    <EditLine>
      <tooltip>cliquer pour crée un object géographique linéare</tooltip>
    </EditLine>
    
    <EditPoint>
      <tooltip>cliquer pour crée un object géographique de point</tooltip>
    </EditPoint>
    
    <Forward>
      <tooltip>get the next extent</tooltip> <!-- TODO: translate -->
    </Forward>
    
    <GetFeatureInfo>
      <tooltip>cliquer pour obtenir plus d'information pour cette couche</tooltip>
    </GetFeatureInfo>
    
    <InsertFeature>
      <tooltip>Save the current Feature(s) to the server</tooltip> <!-- TODO: translate -->
    </InsertFeature>
    
    <Legend>
      <title>Couches de la carte</title>
    </Legend>
    
    <Measurement>
      <tooltip>click on the map to measure the distance</tooltip> <!-- TODO: translate -->
    </Measurement>
    
    <ModelStatus>
      <title>Status for:</title>
    </ModelStatus>
    
    <ModelUrlInput>
      <load>téléchargez</load>
      <title>URL:</title>
    </ModelUrlInput>
    
    <ProcessArgumentsForm>
    </ProcessArgumentsForm>
    
    <Reset>
      <tooltip>redonner la carte ses dimensions complèts</tooltip>
    </Reset>
    
    <Save>
      <tooltip>cliquer pour sauvegarder</tooltip>
    </Save>
    
    <SelectProcess>
    </SelectProcess>
    
    <SetAoi>
      <tooltip>cliquer et faire glisser la souris pour positionner une AOI</tooltip>
    </SetAoi>
    
    <ShowProcessResult>
      <tabLabel>4. Access results</tabLabel> <!-- TODO: translate -->
    </ShowProcessResult>
    
    <TabbedContent>
      <argsInput>3. Set arguments</argsInput> <!-- TODO: translate -->
      <results>4. Access results</results> <!-- TODO: translate -->
      <selectProcess>2. Select a process</selectProcess> <!-- TODO: translate -->
      <urlInput>1. Select a service</urlInput> <!-- TODO: translate -->
    </TabbedContent>
    
    <Timestamp>
      <title>Date de l`image:</title>
    </Timestamp>
    
    <WebServiceForm>
      <mapsheet>code SNRC:</mapsheet>
      <ntsTitle>Service de recherche par SNRC</ntsTitle>
    </WebServiceForm>
    
    <WfsGetFeature>
      <tooltip>Click on feature to query it</tooltip> <!-- TODO: translate -->
    </WfsGetFeature>
    
    <Widget>
      <abstract>Résumé</abstract>
      <east>Est:</east>
      <moreInfo>Lisez à propos de cette carte</moreInfo>
      <north>Nord:</north>
      <searchFormTitle>Région d'intérêt</searchFormTitle>
      <south>Sud:</south>
      <west>Ouest:</west>
    </Widget>
    
    <ZoomIn>
      <tooltip>cliquer et faire glisser la souris pour agrandir</tooltip>
    </ZoomIn>
    
    <ZoomOut>
      <tooltip>cliquer pour réduire</tooltip>
    </ZoomOut>
  </widgets>
</WidgetText>
