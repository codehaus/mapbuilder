<?xml version="1.0" encoding="ISO-8859-1"?><!--Description: parses an OGC context document to generate an array of DHTML layers.Author:      adairLicence:     GPL as specified in http://www.gnu.org/copyleft/gpl.html .$Id: GetFeatureRequest.xsl,v 1.6 2004/10/15 13:42:09 madair1 Exp $$Name:  $--><xsl:stylesheet version="1.0"     xmlns:wps="http://www.opengis.net/wps"    xmlns:xsl="http://www.w3.org/1999/XSL/Transform" 		xmlns:ogc="http://www.opengis.net/ogc"		xmlns:gml="http://www.opengis.net/gml"    xmlns:xlink="http://www.w3.org/1999/xlink">  <xsl:output method="xml" omit-xml-declaration="no" encoding="utf-8" indent="yes"/>  <!-- The coordinates of the DHTML Layer on the HTML page -->  <xsl:param name="modelId"/>  <xsl:param name="widgetId"/>  <xsl:param name="toolId"/>    <!-- template rule matching source root element -->  <xsl:template match="/wps:Capabilities">    <table>      <tr>        <th>          Processes available from: <xsl:value-of select="wps:Service/wps:Title"/>        </th>      </tr>      <xsl:apply-templates select="wps:ProcessOfferings/wps:Process"/>    </table>  </xsl:template>  <!-- template rule matching source root element -->  <xsl:template match="wps:Process">    <xsl:variable name="name"><xsl:value-of select="wps:name"/></xsl:variable>    <tr>      <td>        <xsl:value-of select="$name"/> (Application keywords)      </td>      <td>        <a href="javascript:config.objects.{$modelId}.setParam('wps_DescribeProcess','{$name}')">describe process</a>      </td>    </tr>  </xsl:template>  <xsl:template match="text()|@*"/></xsl:stylesheet>