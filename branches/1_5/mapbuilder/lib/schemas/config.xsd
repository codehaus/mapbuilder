          <xs:element name="moveDownImage" type="mb:urlPathType"/>
          <xs:element name="deleteImage" type="mb:urlPathType"/>
          <xs:element name="title" type="xs:string" minOccurs="0"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
